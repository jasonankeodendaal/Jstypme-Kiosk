
{
  "rewrites": [
    {
      "source": "/manifest-kiosk.json",
      "destination": "/manifest-kiosk.json"
    },
    {
      "source": "/manifest-admin.json",
      "destination": "/manifest-admin.json"
    },
    {
      "source": "/sw.js",
      "destination": "/sw.js"
    },
    {
      "source": "/assets/:path*",
      "destination": "/assets/:path*"
    },
    {
      "source": "/((?!.*\\.(?:js|css|png|jpg|jpeg|gif|svg|ico|pdf|json|woff|woff2|ttf|otf|mp4|webm)).*)",
      "destination": "/index.html"
    }
  ]
}
